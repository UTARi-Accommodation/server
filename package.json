{
    "name": "utari-server",
    "version": "0.0.0",
    "private": true,
    "author": "PoolOfDeath20",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "start": "node build/index.js",
        "start:watch": "nodemon",
        "start:scrapper": "node build/scrapper/index.js",
        "start:dev": "yarn build:dev && yarn start",
        "build": "rm -rf build && tsc -p tsconfig.prod.json && yarn add-js-extension",
        "build:dev": "rm -rf build && tsc -p tsconfig.dev.json && yarn add-js-extension",
        "add-js-extension": "ts-add-js-extension add --dir=build",
        "test:database": "jest test/database/* --run-in-band",
        "test:api": "jest test/api/madm/*",
        "test": "yarn test:database & yarn test:api",
        "code-cov:database": "yarn test:database --coverage --coverageDirectory='coverage/database'",
        "code-cov:api": "yarn test:api --coverage --coverageDirectory='coverage/api'",
        "code-cov": "yarn code-cov:database & yarn code-cov:api"
    },
    "devDependencies": {
        "@babel/plugin-transform-modules-commonjs": "^7.16.8",
        "@babel/preset-env": "^7.16.8",
        "@babel/preset-typescript": "^7.16.7",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.4.0",
        "@types/node": "^17.0.9",
        "@types/pg": "^8.6.3",
        "jest": "^27.4.7",
        "nodemon": "^2.0.15",
        "terser": "^5.10.0",
        "ts-add-js-extension": "^1.0.0",
        "ts-jest": "^27.1.3",
        "typescript": "^4.5.4"
    },
    "dependencies": {
        "dotenv": "^14.2.0",
        "express": "^4.17.2",
        "granula-string": "^1.0.5",
        "node-fetch": "^3.1.1",
        "node-html-parser": "^5.2.0",
        "parse-dont-validate": "^1.0.1",
        "pg": "^8.7.1",
        "puppeteer": "^13.1.2"
    }
}
